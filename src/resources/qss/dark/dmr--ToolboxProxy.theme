dmr--ToolboxProxy[idle="false"] * {
    background: transparent;
    background-color: qlineargradient(x1:0 y1:0, x2:0 y2:1,
        stop:0.00000 transparent,           stop:0.06666 transparent,
        stop:0.06667 rgba(16, 16, 16, 0.8), stop:1.00000 rgba(16, 16, 16, 0.8)
    );
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
} 

dmr--ToolboxProxy[idle="true"] * {
    background: rgba(16, 16, 16, 0.6);
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
}

dmr--ToolboxProxy[idle="false"] Dtk--Widget--DImageButton,
dmr--ToolboxProxy[idle="false"] QLabel {
    background: transparent;
}

dmr--ToolboxProxy[idle="true"] Dtk--Widget--DImageButton,
dmr--ToolboxProxy[idle="true"] QLabel {
    background: transparent;
}

#ToolBoxTopBorder {
    background: transparent;
    border-bottom: 1px inset rgba(0, 0, 0, 0.5);
}

#ToolBoxTopBorderInner {
    background: transparent;
    border-bottom: 1px inset rgba(0, 0, 0, 0.03);
}

