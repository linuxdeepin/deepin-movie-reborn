#MovieProgress {
    background: transparent;
}

#MovieProgress::groove:horizontal {
    background: transparent;
    position: absolute;
    left: 0px; right: 0px;
}
#MovieProgress::handle:horizontal {
    background:transparent;
    margin: 5px -5px;
}
#MovieProgress::add-page:horizontal {
    background: transparent;
}
#MovieProgress::sub-page:horizontal {
    background-color: qlineargradient(x1:0 y1:0, x2:0 y2:1,
        stop:0.00000  transparent,          stop:0.33333 transparent,
        stop:0.33334  #2eacff,              stop:0.50000 #2eacff,
        stop:0.50001  rgba(0, 0, 0, 0.0), stop:1 rgba(0, 0, 0, 0.0)
    );
}

