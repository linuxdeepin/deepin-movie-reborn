dmr--ToolboxProxy, dmr--ToolboxProxy * {
    background: transparent;
    background-color: qlineargradient(x1:0 y1:0, x2:0 y2:1,
        stop:0.00000 transparent,               stop:0.03333 transparent,
        stop:0.03334 rgba(252, 252, 252, 0.88), stop:1.00000 rgba(252, 252, 252, 0.88)
    );
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
}

QLabel, Dtk--Widget--DImageButton {
    background: transparent;
}

#ToolBoxTopBorder {
    background: transparent;
    border-bottom: 1px inset rgba(0, 0, 0, 0.1);
}

#ToolBoxTopBorderInner {
    background: transparent; 
}


